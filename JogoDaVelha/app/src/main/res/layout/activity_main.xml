<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/tres_tres"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:textSize="26sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tres_dois"
        app:layout_constraintTop_toBottomOf="@+id/dois_tres" />

    <Button
        android:id="@+id/tres_dois"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:textSize="26sp"
        app:layout_constraintEnd_toStartOf="@+id/tres_tres"
        app:layout_constraintStart_toEndOf="@+id/tres_um"
        app:layout_constraintTop_toBottomOf="@+id/dois_dois" />

    <Button
        android:id="@+id/tres_um"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:textSize="26sp"
        app:layout_constraintEnd_toStartOf="@+id/tres_dois"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dois_um" />

    <Button
        android:id="@+id/dois_tres"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:textSize="26sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/dois_dois"
        app:layout_constraintTop_toBottomOf="@+id/um_tres" />

    <Button
        android:id="@+id/dois_um"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:textSize="26sp"
        app:layout_constraintEnd_toStartOf="@+id/dois_dois"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/um_um" />

    <Button
        android:id="@+id/dois_dois"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:textSize="26sp"
        app:layout_constraintEnd_toStartOf="@+id/dois_tres"
        app:layout_constraintStart_toEndOf="@+id/dois_um"
        app:layout_constraintTop_toBottomOf="@+id/um_dois" />

    <Button
        android:id="@+id/um_dois"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="100dp"
        android:textSize="26sp"
        app:layout_constraintEnd_toStartOf="@+id/um_tres"
        app:layout_constraintStart_toEndOf="@+id/um_um"
        app:layout_constraintTop_toBottomOf="@+id/Reset" />

    <Button
        android:id="@+id/um_tres"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="100dp"
        android:textSize="26sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/um_dois"
        app:layout_constraintTop_toBottomOf="@+id/Reset" />

    <Button
        android:id="@+id/Reset"
        android:layout_width="408dp"
        android:layout_height="71dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:text="Reset"
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/um_um"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="100dp"
        android:textSize="26sp"
        app:layout_constraintEnd_toStartOf="@+id/um_dois"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Reset" />

    <Button
        android:id="@+id/X"
        android:layout_width="197dp"
        android:layout_height="63dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="5dp"
        android:layout_marginRight="5dp"
        android:text="X"
        android:textSize="36sp"
        app:layout_constraintEnd_toStartOf="@+id/guideline"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Reset" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="205dp" />

    <Button
        android:id="@+id/O"
        android:layout_width="197dp"
        android:layout_height="63dp"
        android:layout_marginStart="5dp"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="20dp"
        android:text="O"
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintStart_toStartOf="@+id/guideline"
        app:layout_constraintTop_toBottomOf="@+id/Reset" />

</androidx.constraintlayout.widget.ConstraintLayout>